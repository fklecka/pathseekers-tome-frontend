sourcesContent":["<template>\n  <div class=\"w-full bg-bg overflow-y-scroll text-font\">\n    <h2 class=\"font-headline text-4xl text-center\">Zusammenfassung</h2>\n    <div class=\"flex gap-12 pt-12\">\n      <div class=\"w-2/4\">\n        <div>\n          <div class=\"flex justify-between\">\n            <p>Name:</p>\n            <p>{{ character.name }}</p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Geschlecht:</p>\n            <p>{{ character.gender }}</p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Volk:</p>\n            <p>{{ character.race }}</p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Klasse:</p>\n            <p>{{ character.classAttributes.classname }}</p>\n          </div>\n        </div>\n        <div class=\"pt-6\">\n          <h3 class=\"text-2xl\">Attributswerte</h3>\n          <hr class=\"pb-3\" />\n          <div class=\"flex justify-between\">\n            <p>Stärke:</p>\n            <p>\n              {{ character.attributes.st + character.raceBonusAttributes.st }}\n              ({{ calcStMod }})\n            </p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Geschicklichkeit:</p>\n            <p>\n              {{ character.attributes.ge + character.raceBonusAttributes.ge }}\n              ({{ calcGeMod }})\n            </p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Konstitution:</p>\n            <p>\n              {{ character.attributes.kon + character.raceBonusAttributes.kon }}\n              ({{ calcKonMod }})\n            </p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Intelligenz:</p>\n            <p>\n              {{ character.attributes.int + character.raceBonusAttributes.int }}\n              ({{ calcIntMod }})\n            </p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Weisheit:</p>\n            <p>\n              {{ character.attributes.wei + character.raceBonusAttributes.wei }}\n              ({{ calcWeiMod }})\n            </p>\n          </div>\n          <div class=\"flex justify-between\">\n            <p>Charisma:</p>\n            <p>\n              {{ character.attributes.ch + character.raceBonusAttributes.ch }}\n              ({{ calcChaMod }})\n            </p>\n          </div>\n        </div>\n      </div>\n      <div class=\"w-2/4\">\n        <h3 class=\"text-2xl\">Erworbene Fähigkeiten</h3>\n        <hr class=\"pb-3\" />\n        <ul class=\"pb-6\">\n          <li v-for=\"(skill, key) in this.character.skills\" :key=\"skill.id\">\n            <p>{{ key }}: {{ skill }}</p>\n          </li>\n        </ul>\n        <h3 class=\"text-2xl\">Erworbene Talente</h3>\n        <hr class=\"pb-3\" />\n        <ul class=\"pb-6\">\n          <li v-for=\"talent in this.character.talents\" :key=\"talent.id\">\n            <p>{{ talent }}</p>\n          </li>\n        </ul>\n        <h3 class=\"text-2xl\">Erworbene Ausrüstung</h3>\n        <hr class=\"pb-3\" />\n        <ul class=\"pb-6\">\n          <li\n            v-for=\"item in this.character.items\"\n            :key=\"item.id\"\n            class=\"list-none\"\n          >\n            <p v-if=\"item.waffe\">{{ item.waffe }}</p>\n            <p v-if=\"item.rüstung\">{{ item.rüstung }}</p>\n          </li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"character\"],\n  created() {\n    console.log(this.character);\n  },\n  computed: {\n    calcStMod() {\n      return Math.floor(\n        (this.character.attributes.st +\n          this.character.raceBonusAttributes.st -\n          10) /\n          2\n      );\n    },\n    calcGeMod() {\n      return Math.floor(\n        (this.character.attributes.ge +\n          this.character.raceBonusAttributes.ge -\n          10) /\n          2\n      );\n    },\n    calcKonMod() {\n      return Math.floor(\n        (this.character.attributes.kon +\n          this.character.raceBonusAttributes.kon -\n          10) /\n          2\n      );\n    },\n    calcIntMod() {\n      return Math.floor(\n        (this.character.attributes.int +\n          this.character.raceBonusAttributes.int -\n          10) /\n          2\n      );\n    },\n    calcWeiMod() {\n      return Math.floor(\n        (this.character.attributes.wei +\n          this.character.raceBonusAttributes.wei -\n          10) /\n          2\n      );\n    },\n    calcChaMod() {\n      return Math.floor(\n        (this.character.attributes.ch +\n          this.character.raceBonusAttributes.ch -\n          10) /\n          2\n      );\n    },\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":""}]}