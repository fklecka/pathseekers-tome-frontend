uD,IAAvD,EAAA,WAAA,EAAuD,gBAAA,CAAA,KAAlB,IAAkB,CAAb,KAAa,CAAvD,EAA+C;AAAA;AAA/C,GACG,CALL,CAWM,EAJN,mBAAA,CAGK,IAHL,EAAA,WAAA,EAGK,CAFH,WAEG,EADH,mBAAA,CAAsD,IAAtD,EAAA,WAAA,EAAsD,gBAAA,CAAA,KAAjB,IAAiB,CAAZ,IAAY,CAAtD,EAA8C;AAAA;AAA9C,GACG,CAHL,CAIM,CAlBR,CA4BI,EATJ,YAAA,CAEC,wBAFD,EAEC;AAFc,IAAA,KAAK,EAAC;AAEpB,GAFD,EAAmC;sBAChC;AAAA,aAAsD,CAAtD,YAAA,CAAsD,sBAAtD,EAAsD;AAAzC,QAAA,EAAE,EAAC;AAAsC,OAAtD,EAAsB;0BAAC;AAAA,iBAAiB,C,WAAA,CAAjB;AAAA,S,CAAD;;;;AAAA,OAAtB,CAAsD,CAAtD;AAAA,K,CADgC;;;;AAAA,GAAnC,CASI,EANJ,YAAA,CAKgB,wBALhB,EAKgB;AAJd,IAAA,KAAK,EAAC,2BAIQ;AAHb,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,cAAA,GAAc,IAAhB;AAAA,KAAA;AAGQ,GALhB,E;sBAGC;AAAA,aAED,C,WAAA,CAFC;AAAA,K;;;;GAHD,CAMI,CA7BN,CAsCI,EARJ,WAQI,EALJ,YAAA,CAGE,mBAHF,EAGE;AAFA,IAAA,KAAK,EAAC,MAEN;AADA,IAAA,IAAI,EAAC;AACL,GAHF,CAKI,EADJ,YAAA,CAAqE,wBAArE,EAAqE;AAAtD,IAAA,KAAK,EAAC;AAAgD,GAArE,EAAmC;sBAAC;AAAA,aAAiB,C,WAAA,CAAjB;AAAA,K,CAAD;;;;AAAA,GAAnC,CACI,CAzCN,CAgDI,EANJ,mBAAA,CAKM,KALN,EAKM,IALN,EAKM,CAJJ,WAII,EADJ,YAAA,CAAiE,wBAAjE,EAAiE;AAAlD,IAAA,KAAK,EAAC;AAA4C,GAAjE,EAAiC;sBAAC;AAAA,aAAe,C,WAAA,CAAf;AAAA,K,CAAD;;;;AAAA,GAAjC,CACI,CALN,CAMI,CAxEN,C","sourcesContent":["<template>\n  <div\n    class=\"\n      w-full\n      h-app\n      bg-bg\n      pt-6\n      md:pt-12\n      pb-36\n      md:pb-16 md:px-12\n      overflow-y-scroll\n    \"\n  >\n    <confirm-modal\n      v-if=\"confirmVisible\"\n      class=\"absolute left-0 top-0 z-50\"\n      @confirm=\"\n        resetPassword();\n        confirmVisible = false;\n      \"\n      @close=\"confirmVisible = false\"\n    >\n      <template v-slot:text>Willst du dein Passwort ändern?</template>\n      <template v-slot:confirm>Password ändern</template>\n    </confirm-modal>\n    <div class=\"w-full xl:w-758 px-2 md:px-0\">\n      <h3 class=\"text-white text-xl pb-6 font-semibold\">Profil</h3>\n      <div class=\"w-32 h-32 rounded-full border bg-white\" />\n      <div class=\"pt-6\">\n        <table class=\"w-full\">\n          <tr class=\"border-b border-opacity-25\">\n            <td class=\"font-semibold text-white opacity-25 w-2/4 py-3\">\n              Benutzername\n            </td>\n            <td class=\"text-white w-2/4\">{{ this.user.username }}</td>\n          </tr>\n          <tr class=\"border-b border-opacity-25\">\n            <td class=\"font-semibold text-white opacity-25 w-2/4 py-3\">\n              E-Mail\n            </td>\n            <td class=\"text-white w-2/4\">{{ this.user.email }}</td>\n          </tr>\n\n          <tr class=\"border-b border-opacity-25\">\n            <td class=\"font-semibold text-white opacity-25 w-2/4 py-3\">Name</td>\n            <td class=\"text-white w-2/4\">{{ this.user.name }}</td>\n          </tr>\n        </table>\n        <custom-button class=\"bg-card mt-6\"\n          ><router-link to=\"edit\">Profil bearbeiten</router-link></custom-button\n        >\n        <custom-button\n          class=\"bg-card mt-6 ml-2 md:ml-6\"\n          @click=\"confirmVisible = true\"\n        >\n          Passwort ändern\n        </custom-button>\n      </div>\n      <h3 class=\"text-white text-xl pt-12 pb-6 font-semibold\">\n        Dein Abonnement\n      </h3>\n      <abo-card\n        title=\"Free\"\n        text=\"Nutze Pathseeker's Tome gratis mit beschränkter Anzahl an Charakter-Slots\"\n      />\n      <custom-button class=\"bg-card mt-6\">Abonnement ändern</custom-button>\n    </div>\n    <div>\n      <h3 class=\"text-white text-xl pb-6 pt-12 font-semibold\">\n        Account löschen\n      </h3>\n      <custom-button class=\"bg-red-600\">Account löschen</custom-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CustomButton from \"../../components/CustomButton.vue\";\nimport AboCard from \"../../components/AboCard.vue\";\nimport { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport ConfirmModal from \"../../components/ConfirmModal.vue\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  name: \"AccountOverview\",\n  components: { CustomButton, AboCard, ConfirmModal },\n  data: () => {\n    return {\n      confirmVisible: false,\n      errors: {},\n    };\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n  methods: {\n    async resetPassword() {\n      const toast = useToast();\n      const request = {};\n      request.email = this.user.email;\n      try {\n        axios.post(\"http://localhost/api/password/email\", request);\n      } catch ({ data }) {\n        this.errors = data.errors;\n      } finally {\n        this.$store.state.isLoading = false;\n        toast.success(\"Login erfolgreich!\");\n        console.log(this.errors);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""}]}