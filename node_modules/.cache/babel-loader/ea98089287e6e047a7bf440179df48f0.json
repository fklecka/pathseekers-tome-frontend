b-6\n                h-10\n                px-5\n                text-xl\n                border\n                email\n                bg-bg\n                rounded-2xl\n              \"\n              name=\"newPassword\"\n              id=\"newPassword\"\n            />\n          </div>\n          <div class=\"flex flex-col\">\n            <label for=\"confirmNewPassword\" class=\"text-xl\">\n              Neues Password bestätigen\n            </label>\n            <input\n              type=\"text\"\n              class=\"\n                w-full\n                mb-6\n                h-10\n                px-5\n                text-xl\n                border\n                email\n                bg-bg\n                rounded-2xl\n              \"\n              name=\"confirmNewPassword\"\n              id=\"confirmNewPassword\"\n            />\n          </div>\n          <custom-button class=\"bg-green\" @click=\"submit\"\n            >Neues Passwort speichern</custom-button\n          >\n          <router-link to=\"overview\" class=\"ml-6\">abbrechen</router-link>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CustomButton from \"../../components/CustomButton.vue\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  name: \"AccountEdit\",\n  components: { CustomButton },\n  data: () => {\n    return {\n      form: {\n        email: \"\",\n        password: \"\",\n        password_confirmation: \"\",\n      },\n    };\n  },\n  methods: {\n    ...mapActions({\n      resetPassword: \"auth/resetPassword\",\n    }),\n    submit() {\n      const toast = useToast();\n      try {\n        this.$store.state.isLoading = true;\n        this.resetPassword(this.form);\n      } catch ({ data }) {\n        console.log(data);\n        this.errors = data.errors;\n      } finally {\n        this.$store.state.isLoading = false;\n\n        toast.success(\"Password erfolgreich geändert!\");\n      }\n    },\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":""}]}