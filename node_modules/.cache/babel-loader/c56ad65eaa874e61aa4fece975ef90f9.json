ue;\n      const apiUrl = \"http://localhost/api/characters/show/\" + this.user.id;\n      try {\n        const response = await axios.get(apiUrl);\n        console.log(response.data);\n        this.characters = response.data;\n      } catch (e) {\n        this.errors.push(e);\n      } finally {\n        this.$store.state.isLoading = false;\n      }\n    } else {\n      let localObject = { characters: {} };\n      this.localObject.characters = await JSON.parse(\n        localStorage.getItem(\"characters\")\n      );\n      this.characters = localObject;\n    }\n  },\n};\n</script>\n\n<style scoped>\n::-webkit-scrollbar {\n  width: 5px;\n}\n::-webkit-scrollbar-thumb {\n  background: rgba(256, 256, 256, 0.2);\n}\n</style>\n"],"sourceRoot":""}]}