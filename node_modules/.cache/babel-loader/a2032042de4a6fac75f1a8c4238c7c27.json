lt {\n  components: { searchBar, weaponList },\n  data() {\n    return {\n      searchKeyword: \"\",\n      lightWeapons: [],\n      warWeapons: [],\n      exoticWeapons: [],\n      errors: [],\n    };\n  },\n  methods: {\n    getKeyword(keyword) {\n      this.searchKeyword = keyword;\n    },\n  },\n  async created() {\n    try {\n      this.$store.state.isLoading = true;\n      const response = await axios.get(\"http://localhost/api/einfachewaffen\");\n      this.lightWeapons = response.data;\n    } catch (e) {\n      this.errors.push(e);\n    } finally {\n      this.$store.state.isLoading = false;\n    }\n    try {\n      this.$store.state.isLoading = true;\n      const response = await axios.get(\"http://localhost/api/kriegswaffen\");\n      this.warWeapons = response.data;\n    } catch (e) {\n      this.errors.push(e);\n    } finally {\n      this.$store.state.isLoading = false;\n    }\n    try {\n      this.$store.state.isLoading = true;\n      const response = await axios.get(\"http://localhost/api/exotischewaffen\");\n      this.exoticWeapons = response.data;\n    } catch (e) {\n      this.errors.push(e);\n    } finally {\n      this.$store.state.isLoading = false;\n    }\n  },\n};\n</script>\n\n<style scoped>\na:hover {\n  color: #f28241;\n}\n</style>\n"],"sourceRoot":""}]}