;AAAA;AAAA;AAAA;AAAA;AAAA;AACd,gBAAA,KADc,GACN,QAAQ,EADF;AAAA;AAAA,uBAEd,KAAI,CACP,IADG,CACE,qBADF,EACyB,KAAI,CAAC,IAD9B,EAEH,IAFG,CAEE,YAAM;AACV,kBAAA,KAAK,CAAC,OAAN,CAAc,uBAAd;;AACA,kBAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,QAAlB;AACD,iBALG,EAMH,KANG,CAMG,UAAC,CAAD,EAAO;AACZ,kDAAoB,MAAM,CAAC,IAAP,CAAY,CAAC,CAAC,QAAF,CAAW,IAAX,CAAgB,MAA5B,CAApB,kCAAyD;AAApD,wBAAM,KAAI,mBAAV;AACH,oBAAA,KAAK,CAAC,KAAN,CAAY,CAAC,CAAC,QAAF,CAAW,IAAX,CAAgB,MAAhB,CAAuB,KAAvB,EAA8B,CAA9B,CAAZ;AACF;AACD,iBAVG,CAFc;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAarB;AAdM;AATI,CAAf","sourcesContent":["<template>\n  <div class=\"w-full h-full pb-32 bg-bg flex justify-center items-center\">\n    <div class=\"m-auto w-screen px-4 sm:w-400 pt-50\">\n      <img src=\"../../assets/tome_logo_white.svg\" alt=\"\" />\n      <h2 class=\"text-4xl font-semibold text-font pt-50\">Passwort vergessen</h2>\n      <div class=\"pt-12 text-font\">\n        <form action=\"\" @submit.prevent=\"resetPassword\">\n          <div class=\"flex flex-col\">\n            <input\n              type=\"email\"\n              name=\"email\"\n              id=\"email\"\n              placeholder=\"E-Mail\"\n              class=\"\n                w-full\n                h-10\n                px-5\n                mt-8\n                text-xl\n                border\n                email\n                bg-bg\n                border-orange\n                rounded-2xl\n                text-font\n              \"\n              v-model=\"form.email\"\n            />\n          </div>\n          <div>\n            <button\n              type=\"submit\"\n              class=\"\n                w-full\n                h-12\n                text-xl\n                font-bold\n                text-white\n                border-0\n                rounded-2xl\n                bg-orange\n                mt-7\n              \"\n            >\n              <p v-if=\"this.$store.state.isLoading === false\">\n                Passwort zur√ºcksetzen\n              </p>\n              <p v-if=\"this.$store.state.isLoading === true\">Bitte warten</p>\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  data() {\n    return {\n      form: {\n        email: \"\",\n      },\n      errors: {},\n    };\n  },\n  methods: {\n    async resetPassword() {\n      const toast = useToast();\n      await axios\n        .post(\"/api/password/email\", this.form)\n        .then(() => {\n          toast.success(\"Email wurde versendet\");\n          this.$router.push(\"/login\");\n        })\n        .catch((e) => {\n          for (const error of Object.keys(e.response.data.errors)) {\n            toast.error(e.response.data.errors[error][0]);\n          }\n        });\n    },\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":""}]}