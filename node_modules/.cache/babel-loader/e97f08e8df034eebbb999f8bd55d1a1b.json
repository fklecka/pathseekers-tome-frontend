n      \"\n      v-if=\"!this.user\"\n    >\n      <p>Um einen Charakter zu erstellen ben√∂tigen Sie einen Account</p>\n      <p class=\"pt-6\">\n        Bitte loggen Sie sich ein oder registrieren einen neuen Account\n      </p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CharacterCard from \"../components/CharacterCard.vue\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"CharacterOverview\",\n  components: { CharacterCard },\n  data: () => {\n    return {\n      characters: [],\n      errors: [],\n    };\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n  async created() {\n    if (this.user) {\n      this.$store.state.isLoading = true;\n      const apiUrl = \"http://localhost/api/characters/show/\" + this.user.id;\n      try {\n        const response = await axios.get(apiUrl);\n        console.log(response.data);\n        this.$store.state.characters = response.data;\n      } catch (e) {\n        this.errors.push(e);\n      } finally {\n        this.$store.state.isLoading = false;\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\n::-webkit-scrollbar {\n  width: 5px;\n}\n::-webkit-scrollbar-thumb {\n  background: rgba(256, 256, 256, 0.2);\n}\n</style>\n"],"sourceRoot":""}]}