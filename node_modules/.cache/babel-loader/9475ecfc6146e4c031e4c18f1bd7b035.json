sword {\n  background-image: url(\"../assets/password.svg\");\n}\n.email {\n  background-image: url(\"../assets/user.svg\");\n}\ninput[type=\"checkbox\"] {\n  -webkit-appearance: none;\n  appearance: none;\n  background-color: background-color;\n  margin: 0;\n  width: 1.5rem;\n  height: 1.5rem;\n  border: 1px solid rgb(255, 255, 255, 0.87);\n  border-radius: 0.15em;\n  transform: translateY(0.075em);\n  display: grid;\n  place-content: center;\n}\ninput[type=\"checkbox\"]::before {\n  content: \"\";\n  width: 0.8rem;\n  height: 0.8rem;\n  transform: scale(0);\n  transition: 120ms transform ease-in-out;\n  box-shadow: inset 1em 1em var(--form-control-color);\n  background-color: rgb(255, 255, 255, 0.87);\n  transform-origin: bottom left;\n  clip-path: polygon(\n    20% 0%,\n    0% 20%,\n    30% 50%,\n    0% 80%,\n    20% 100%,\n    50% 70%,\n    80% 100%,\n    100% 80%,\n    70% 50%,\n    100% 20%,\n    80% 0%,\n    50% 30%\n  );\n}\ninput[type=\"checkbox\"]:checked::before {\n  transform: scale(1);\n}\n.checkbox-class {\n  display: grid;\n  grid-template-columns: 1em auto;\n  gap: 0.5em;\n  font-weight: 400;\n}\n</style>\n\n<script>\nimport { mapActions } from \"vuex\";\nexport default {\n  data() {\n    return {\n      pwtype: \"password\",\n      showButton: \"Anzeigen\",\n      form: {\n        email: \"\",\n        password: \"\",\n      },\n    };\n  },\n  methods: {\n    showPassword(e) {\n      e.preventDefault();\n      if (this.pwtype === \"password\") {\n        this.pwtype = \"text\";\n        this.showButton = \"Verstecken\";\n      } else {\n        this.pwtype = \"password\";\n        this.showButton = \"Anzeigen\";\n      }\n    },\n    ...mapActions({\n      login: \"auth/login\",\n    }),\n    async submit() {\n      await this.login(this.form);\n\n      this.$router.replace({ name: \"CharacterOverview\" });\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}