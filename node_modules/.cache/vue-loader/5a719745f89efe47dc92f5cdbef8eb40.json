              class=\"\n                w-full\n                h-10\n                px-5\n                text-xl\n                border\n                email\n                bg-bg\n                border-orange\n                rounded-2xl\n                text-font\n              \"\n              v-model=\"form.username\"\n            />\n            <span class=\"text-orange\" v-if=\"this.errors.username\">{{\n              this.errors.username.toString()\n            }}</span>\n          </div>\n          <div>\n            <input\n              type=\"email\"\n              name=\"email\"\n              id=\"email\"\n              placeholder=\"E-Mail\"\n              class=\"\n                w-full\n                h-10\n                px-5\n                mt-8\n                text-xl\n                border\n                email\n                bg-bg\n                border-orange\n                rounded-2xl\n                text-font\n              \"\n              v-model=\"form.email\"\n            />\n            <span class=\"text-orange\" v-if=\"this.errors.email\">{{\n              this.errors.email.toString()\n            }}</span>\n          </div>\n          <div class=\"relative\">\n            <input\n              :type=\"pwType\"\n              name=\"password\"\n              id=\"password\"\n              placeholder=\"Passwort\"\n              class=\"\n                w-full\n                h-10\n                px-5\n                mt-8\n                text-xl\n                border\n                password\n                bg-bg\n                border-orange\n                rounded-2xl\n                text-font\n              \"\n              v-model=\"form.password\"\n            />\n            <button\n              class=\"\n                absolute\n                text-xl\n                border-0\n                text-font\n                opacity-60\n                top-38\n                right-5\n              \"\n              @click=\"showPassword\"\n            >\n              {{ showButton }}\n            </button>\n          </div>\n          <div class=\"relative\">\n            <input\n              :type=\"pwTypeConfirm\"\n              name=\"confirmpassword\"\n              id=\"confirmpassword\"\n              placeholder=\"Passwort bestätigen\"\n              class=\"\n                w-full\n                h-10\n                px-5\n                mt-8\n                text-xl\n                border\n                password\n                bg-bg\n                border-orange\n                rounded-2xl\n                text-font\n              \"\n              v-model=\"form.password_confirmation\"\n            />\n            <button\n              class=\"\n                absolute\n                text-xl\n                border-0\n                text-font\n                opacity-60\n                top-38\n                right-5\n              \"\n              @click=\"showPasswordConfirm\"\n            >\n              {{ showButtonConfirm }}\n            </button>\n          </div>\n          <div\n            class=\"\n              flex\n              justify-between\n              px-3\n              text-lg\n              items\n              center\n              text-orange\n              mt-7\n            \"\n          >\n            <div>\n              <label for=\"accept\" class=\"checkbox-class text-font\">\n                <input\n                  type=\"checkbox\"\n                  name=\"accept\"\n                  id=\"accept\"\n                  class=\"form-checkbox border-orange\"\n                />\n                Ich habe die Datenschutzerklärung gelesen und akzeptiert</label\n              >\n            </div>\n          </div>\n          <div>\n            <button\n              type=\"submit\"\n              class=\"\n                w-full\n                h-12\n                text-xl\n                font-bold\n                text-white\n                border-0\n                rounded-2xl\n                bg-orange\n                mt-7\n              \"\n            >\n              <p v-if=\"processing === false\">Registrieren</p>\n              <p v-if=\"processing === true\">Bitte warten</p>\n            </button>\n          </div>\n        </form>\n        <p class=\"bottom-4 text-lg text-center text-white opacity-50 pt-14\">\n          Sie haben bereits einen Account?\n          <router-link to=\"/login\">\n            <span class=\"text-orange\">Login</span></router-link\n          >\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<style scoped>\ninput[type=\"checkbox\"] {\n  -webkit-appearance: none;\n  appearance: none;\n  background-color: background-color;\n  margin: 0;\n  width: 1.5rem;\n  height: 1.5rem;\n  border: 1px solid rgb(255, 255, 255, 0.87);\n  border-radius: 0.15em;\n  transform: translateY(0.075em);\n  display: grid;\n  place-content: center;\n}\ninput[type=\"checkbox\"]::before {\n  content: \"\";\n  width: 0.8rem;\n  height: 0.8rem;\n  transform: scale(0);\n  transition: 120ms transform ease-in-out;\n  box-shadow: inset 1em 1em var(--form-control-color);\n  background-color: rgb(255, 255, 255, 0.87);\n  transform-origin: bottom left;\n  clip-path: polygon(\n    20% 0%,\n    0% 20%,\n    30% 50%,\n    0% 80%,\n    20% 100%,\n    50% 70%,\n    80% 100%,\n    100% 80%,\n    70% 50%,\n    100% 20%,\n    80% 0%,\n    50% 30%\n  );\n}\ninput[type=\"checkbox\"]:checked::before {\n  transform: scale(1);\n}\n.checkbox-class {\n  display: grid;\n  grid-template-columns: 1em auto;\n  gap: 0.5em;\n  font-weight: 400;\n}\n</style>\n\n<script>\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"register\",\n  data() {\n    return {\n      pwType: \"password\",\n      pwTypeConfirm: \"password\",\n      showButton: \"Anzeigen\",\n      showButtonConfirm: \"Anzeigen\",\n      processing: false,\n      form: {\n        username: \"test\",\n        email: \"test@test.com\",\n        password: \"password123\",\n        password_confirmation: \"password123\",\n      },\n      errors: {},\n    };\n  },\n  methods: {\n    showPassword(e) {\n      e.preventDefault();\n      if (this.pwType === \"password\") {\n        this.pwType = \"text\";\n        this.showButton = \"Verstecken\";\n      } else {\n        this.pwType = \"password\";\n        this.showButton = \"Anzeigen\";\n      }\n    },\n    showPasswordConfirm(e) {\n      e.preventDefault();\n      if (this.pwTypeConfirm === \"password\") {\n        this.pwTypeConfirm = \"text\";\n        this.showButtonConfirm = \"Verstecken\";\n      } else {\n        this.pwTypeConfirm = \"password\";\n        this.showButtonConfirm = \"Anzeigen\";\n      }\n    },\n    ...mapActions({\n      register: \"auth/register\",\n    }),\n    async submit() {\n      try {\n        this.processing = true;\n        await this.register(this.form);\n        this.$router.replace({ name: \"CharacterOverview\" });\n      } catch ({ data }) {\n        this.errors = data.errors;\n      } finally {\n        this.processing = false;\n      }\n    },\n  },\n};\n</script>\n"]}]}