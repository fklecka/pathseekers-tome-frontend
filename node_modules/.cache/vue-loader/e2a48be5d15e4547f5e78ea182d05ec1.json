      />\n      <div class=\"w-758 px-6\">\n        <intro v-if=\"this.step === 0\" />\n        <step-one\n          v-if=\"this.step === 1\"\n          @passName=\"getName($event)\"\n          @setAndPassGender=\"getGender($event)\"\n        />\n        <step-two\n          v-if=\"this.step === 2\"\n          @passAttributes=\"getAttributes($event)\"\n        />\n        <step-three v-if=\"this.step === 3\" @passRace=\"getRace($event)\" />\n        <step-four v-if=\"this.step === 4\" @passClass=\"getClass($event)\" />\n        <step-five\n          v-if=\"this.step === 5\"\n          :class=\"this.character.classAttributes.classname\"\n          :character=\"this.character\"\n          @passSkills=\"getSkills($event)\"\n        />\n        <step-six v-if=\"this.step === 6\" @passTalents=\"getTalents($event)\" />\n        <step-seven\n          v-if=\"this.step === 7\"\n          @passItems=\"getItems($event)\"\n          :money=\"this.character.money\"\n        />\n        <step-eight v-if=\"this.step === 8\" :character=\"this.character\" />\n        <div class=\"relative pt-6\">\n          <custom-button\n            v-if=\"step > 0\"\n            class=\"bg-card border border-card hover:bg-bg absolute left-0\"\n            @click=\"this.step -= 1\"\n            >Zurück</custom-button\n          >\n          <custom-button\n            v-if=\"step < 8\"\n            class=\"bg-card border border-card hover:bg-bg absolute right-0\"\n            @click=\"this.step += 1\"\n            >Weiter</custom-button\n          >\n          <custom-button\n            v-if=\"step === 8\"\n            class=\"bg-card border border-card hover:bg-bg absolute right-0\"\n            @click=\"saveCharacter(this.character)\"\n            >Charakter Speichern</custom-button\n          >\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CustomButton from \"../components/CustomButton.vue\";\nimport InfoBox from \"../components/charactertool/InfoBox.vue\";\nimport Intro from \"../components/charactertool/Intro.vue\";\nimport StepOne from \"../components/charactertool/StepOne.vue\";\nimport StepTwo from \"../components/charactertool/StepTwo.vue\";\nimport StepThree from \"../components/charactertool/StepThree.vue\";\nimport StepFour from \"../components/charactertool/StepFour.vue\";\nimport StepFive from \"../components/charactertool/StepFive.vue\";\nimport StepSix from \"../components/charactertool/StepSix.vue\";\nimport StepSeven from \"../components/charactertool/StepSeven.vue\";\nimport StepEight from \"../components/charactertool/StepEight.vue\";\nimport { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nimport { useToast } from \"vue-toastification\";\nexport default {\n  data: () => {\n    return {\n      step: 0,\n      character: {\n        name: \"\",\n        gender: \"\",\n        race: \"\",\n        sizeMod: 0,\n        speed: 0,\n        money: 0,\n        attributes: {\n          st: 10,\n          ge: 10,\n          kon: 10,\n          int: 10,\n          wei: 10,\n          ch: 10,\n        },\n        raceBonusAttributes: {\n          st: 0,\n          ge: 0,\n          kon: 0,\n          int: 0,\n          wei: 0,\n          ch: 0,\n        },\n        classAttributes: {\n          classname: \"\",\n          favoredBonus: \"\",\n          hp: 0,\n          bonusHp: 0,\n          bonusSkill: 0,\n          skillPoints: 0,\n          ref: 0,\n          wil: 0,\n          zah: 0,\n          gab: 0,\n        },\n        skills: {},\n        talents: [],\n        items: [],\n      },\n    };\n  },\n  components: {\n    CustomButton,\n    InfoBox,\n    Intro,\n    StepOne,\n    StepTwo,\n    StepThree,\n    StepFour,\n    StepFive,\n    StepSix,\n    StepSeven,\n    StepEight,\n  },\n  computed: {\n    ...mapGetters({\n      user: \"auth/user\",\n    }),\n  },\n  methods: {\n    getName(name) {\n      this.character.name = name;\n    },\n    getGender(gender) {\n      this.character.gender = gender;\n    },\n    getRace(race) {\n      this.character.race = race.name;\n      this.character.raceBonusAttributes = race.bonusAttributes;\n      this.character.sizeMod = race.sizeMod;\n      this.character.speed = race.speed;\n    },\n    getAttributes(attributes) {\n      this.character.attributes = attributes;\n    },\n    getClass(classInfo) {\n      this.character.classAttributes.classname = classInfo.classname;\n      this.character.classAttributes.favoredBonus = classInfo.favoredBonus;\n      this.character.classAttributes.hp = classInfo.hp;\n      this.character.classAttributes.bonusHp = classInfo.bonusHp;\n      this.character.classAttributes.bonusSkill = classInfo.bonusSkill;\n      this.character.classAttributes.skillPoints = classInfo.skillPoints;\n      this.character.classAttributes.ref = classInfo.ref;\n      this.character.classAttributes.wil = classInfo.wil;\n      this.character.classAttributes.zah = classInfo.zah;\n      this.character.classAttributes.gab = classInfo.gab;\n      this.character.money = classInfo.money;\n    },\n    getSkills(skills) {\n      this.character.skills = skills;\n      console.log(this.character.skills);\n    },\n    getTalents(talents) {\n      this.character.talents = talents;\n    },\n    getItems(items) {\n      this.character.items = items;\n    },\n    async saveCharacter(character) {\n      const toast = useToast();\n      this.$store.state.isLoading = true;\n      const request = { character: character, user_id: this.user.id };\n      await axios\n        .post(\"/api/character/store\", request)\n        .then(({ data }) => {\n          console.log(data.message);\n          this.$router.replace({ name: \"CharacterOverview\" });\n        })\n        .catch(({ response: data }) => {\n          throw data;\n        })\n        .finally(() => {\n          this.$store.state.isLoading = false;\n          toast.success(\"Charakter gespeichert!\");\n        });\n    },\n  },\n};\n</script>\n\n<style></style>\n"]}]}