,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC;QACpE,CAAC;MACH,CAAC,CAAC;IACJ,CAAC;EACH,CAAC;EACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;AACb,CAAC","file":"/Users/florianklecka/Documents/SAE - WEB DESIGN/DIPLOMPROJEKT/Pathseekers Tome/frontend/src/components/kompendium/WeaponList.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <table class=\"overflow-x-scroll\">\n      <tr class=\"text-left\">\n        <th>Waffe</th>\n        <th>Preis</th>\n        <th>Schaden (K)</th>\n        <th>Schaden (M)</th>\n        <th>Kritischer Treffer</th>\n        <th>Grundreichweite</th>\n        <th>Gewicht</th>\n        <th>Art</th>\n        <th>Speziell</th>\n      </tr>\n      <tr>\n        <td class=\"font-bold col text-center border\" colspan=\"9\">\n          Einfache Waffen\n        </td>\n      </tr>\n      <tr v-for=\"item in lightWeaponsfilteredData\" :key=\"item.id\">\n        <td>{{ item.waffe }}</td>\n        <td>{{ item.preis }}</td>\n        <td>{{ item.schaden_k }}</td>\n        <td>{{ item.schaden_m }}</td>\n        <td>{{ item.kritischer_treffer }}</td>\n        <td>{{ item.grundreichweite }}</td>\n        <td>{{ item.gewicht }}</td>\n        <td>{{ item.art }}</td>\n        <td>{{ item.speziell }}</td>\n      </tr>\n      <tr>\n        <td class=\"font-bold col text-center border\" colspan=\"9\">\n          Kriegswaffen\n        </td>\n      </tr>\n      <tr v-for=\"item in warWeaponsfilteredData\" :key=\"item.id\">\n        <td>{{ item.waffe }}</td>\n        <td>{{ item.preis }}</td>\n        <td>{{ item.schaden_k }}</td>\n        <td>{{ item.schaden_m }}</td>\n        <td>{{ item.kritischer_treffer }}</td>\n        <td>{{ item.grundreichweite }}</td>\n        <td>{{ item.gewicht }}</td>\n        <td>{{ item.art }}</td>\n        <td>{{ item.speziell }}</td>\n      </tr>\n      <tr>\n        <td class=\"font-bold col text-center border\" colspan=\"9\">\n          Exotische Waffen\n        </td>\n      </tr>\n      <tr v-for=\"item in exoticWeaponsfilteredData\" :key=\"item.id\">\n        <td>{{ item.waffe }}</td>\n        <td>{{ item.preis }}</td>\n        <td>{{ item.schaden_k }}</td>\n        <td>{{ item.schaden_m }}</td>\n        <td>{{ item.kritischer_treffer }}</td>\n        <td>{{ item.grundreichweite }}</td>\n        <td>{{ item.gewicht }}</td>\n        <td>{{ item.art }}</td>\n        <td>{{ item.speziell }}</td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"lightWeapons\", \"exoticWeapons\", \"warWeapons\", \"searchKey\"],\n  data() {\n    return {};\n  },\n  computed: {\n    lightWeaponsfilteredData() {\n      return this.lightWeapons.filter((item) => {\n        return (\n          item.waffe.toLowerCase().indexOf(this.searchKey.toLowerCase()) > -1\n        );\n      });\n    },\n    warWeaponsfilteredData() {\n      return this.warWeapons.filter((item) => {\n        return (\n          item.waffe.toLowerCase().indexOf(this.searchKey.toLowerCase()) > -1\n        );\n      });\n    },\n    exoticWeaponsfilteredData() {\n      return this.exoticWeapons.filter((item) => {\n        return (\n          item.waffe.toLowerCase().indexOf(this.searchKey.toLowerCase()) > -1\n        );\n      });\n    },\n  },\n  methods: {},\n};\n</script>\n\n<style scoped>\nth,\ntd {\n  padding: 10px;\n}\ntr:nth-child(even) {\n  background-color: rgba(255, 255, 255, 0.05);\n}\ntable {\n  table-layout: fixed;\n}\n</style>\n"]}]}